  <div class="prog">
  <article>
    <h2>Codificação vhdl</h2>
    <div class="form">
        <form class="container" method="get" action="/program">
            <div class="form-group">
                <textarea class="codemirror-textarea" id="code" name="codigo"></textarea>
            </div>
            <button type="submit" onclick="alertar()" class="play"><img src="/lab-fpga/img/run.svg" alt="Icone start"></button>
            <script>
                var editor = CodeMirror.fromTextArea(document.getElementById("code"), {
                    lineNumbers: true,
                    matchBrackets: true,
                    mode: {
                        name: "vhdl"
                    }
                });
                var codigo = editor.getValue();

                function alertar(){
                    alert("Codigo enviado com sucesso!");
                }
                
            </script>
        </form>
        
