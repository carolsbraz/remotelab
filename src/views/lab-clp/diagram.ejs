    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="/general/js/instance-axios.js"></script>
    <script src="/general/js/navbar.js"></script>
    <script src="/lab-clp/js/diagram.js"></script>
    <script src="/general/js/animations/load.js"></script>



</body>

</html>